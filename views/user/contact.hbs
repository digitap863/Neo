                                        <input type="text" class="form-control primary-bg2" name="subject" id="subject" placeholder="Subject*" >
                                    </div>
                                    <div class="comment mb-25">
                                        <textarea name="message" class="form-control primary-bg2" id="message" placeholder="Comments"></textarea>
                                    </div>
                                    <a href="#" class="web-btn d-inline-block theme-bg white text-uppercase over-hidden position-relative border-theme02 ptb-10 pl-40 pr-40">
